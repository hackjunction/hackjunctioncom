    "timedescription": {
      "default": "",
      "type": "string"
    },
    "locationdescription": {
      "default": "",
      "type": "string"
    },
    "shortdescription": {
      "default": "",
      "type": "text"
    },
    "longdescription": {
      "default": "",
      "type": "text",
      "required": true
    },
    "eventcategory": {
      "model": "eventcategory",
      "via": "eventconcepts"
    }
  }
}